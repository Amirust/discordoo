{
  "name": "discordoo",
  "version": "1.0.0",
  "description": "",
  "exports": {
    "./core": "./dist/src/core/index.js",
    "./cache": "./dist/src/cache/index.js",
    "./collection": "./dist/src/dist/collection/index.js",
    "./rest": "./dist/src/rest/index.js",
    "./util": "./dist/src/util/index.js",
    "./main": "./dist/src/main/index.js",
    "./ws": "./dist/src/ws/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "core": [
        "types/core/index.d.ts"
      ],
      "cache": [
        "types/cache/index.d.ts"
      ],
      "collection": [
        "types/collection/index.d.ts"
      ],
      "rest": [
        "types/rest/index.d.ts"
      ],
      "util": [
        "types/util/index.d.ts"
      ],
      "ws": [
        "types/ws/index.d.ts"
      ],
      "main": [
        "types/main/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "docs": "typedoc",
    "utils": "node scripts/root.js"
  },
  "keywords": [],
  "contributors": [
    "All the amazing people from https://github.com/Discordoo/discordoo/graphs/contributors"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/node-fetch": "^2.5.9",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ts-jest": "^26.5.4",
    "tscpaths": "^0.0.9",
    "typedoc": "^0.20.32",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint src --fix"
    ]
  },
  "dependencies": {
    "discord-api-types": "^0.12.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "tiny-typed-emitter": "^2.0.3"
