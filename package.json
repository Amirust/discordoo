{
  "name": "discordoo",
  "version": "0.9.0",
  "description": "Discord bots. Simplified and boosted",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./types/src/index.d.ts",
  "scripts": {
    "lint": "eslint tests src",
    "lint:fix": "eslint tests src --fix",
    "test": "jest",
    "test:fly": "node dist/on-the-fly/index.js",
    "test:fly:nsolid": "npx nsolid-quickstart --exec dist/on-the-fly/index.js",
    "build": "ttsc -p tsconfig.json",
    "build:publish": "npx rimraf dist types && ttsc -p tsconfig.publish.json",
    "build:watch": "ttsc -p tsconfig.json --watch",
    "docs": "npx typedoc && npx tapok docs/docs/api",
    "docs:searchfix": "npx tapok docs/site/search/search_index.json --mode search",
    "docs:collection": "cd ./__collection__ && npm ci && npm run docs",
    "prepublishOnly": "npm run build:publish",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": {
    "name": "Mirdukkk",
    "email": "dev@discordoo.xyz"
  },
  "contributors": [
    "All the amazing people from https://github.com/Discordoo/discordoo/graphs/contributors"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Discordoo/discordoo.git"
  },
  "homepage": "https://discordoo.xyz/",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.0.1",
    "@discordoo/tapok": "^1.0.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.0.0",
    "@types/node-ipc": "^9.1.4",
    "@types/pako": "^1.0.1",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^7.22.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.22.6",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@discordoo/collection": "^1.0.11",
    "@discordoo/providers": "^0.8.2",
    "node-ipc": "^9.1.4",
    "tiny-typed-emitter": "2.0.3",
    "tslib": "^2.3.0",
    "typescript-is": "^0.18.3",
    "undici": "^4.4.6",
    "ws": "^7.4.5"
  },
  "engines": {
    "node": ">=12.18"
  },
  "publishConfig": {
    "access": "public"
  }
}
